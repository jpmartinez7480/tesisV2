hampelFilter<-function(signal,window,threshold){
  library(jsonlite)
  library(pracma)
  #signal <- '[[31,30,29,27,29,19],[38,37,40,31,30,30],[57.81,57.546,57.81,57.546,57.072,57.072],[7.6,8.2,8.2,8.2,8.2,8.2]]'
  signal <- fromJSON(signal, simplifyVector = TRUE)
  final <- list()
  i <- 1
  while( i < 5){
    result <- hampel(signal[i,], k=window,t0 = threshold)
    final[i] <- result
    i <- i + 1
  }
  return (final)
}
